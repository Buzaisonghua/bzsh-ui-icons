{
  "name": "bzsh-icon",
  "version": "0.9.0",
  "description": "基于 Remix icon 生成的 Vue3 图标组件库",
  "keywords": [
    "Vue3",
    "icon",
    "remixicon"
  ],
  "author": "Sam",
  "license": "MIT",
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./es": {
      "types": "./types/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs"
    },
    "./es/*.mjs": {
      "types": "./types/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./types/*.d.ts",
        "./types/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.cjs": {
      "types": "./types/*.d.ts",
      "require": "./lib/*.cjs"
    },
    "./lib/*": {
      "types": [
        "./types/*.d.ts",
        "./types/*/index.d.ts"
      ],
      "require": "./lib/*.cjs"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "types",
    "global.d.ts",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@vueuse/components": "^11.1.0",
    "@vueuse/core": "^10.11.0"
  },
  "devDependencies": {
    "vue": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Buzaisonghua/bzsh-ui-icons.git"
  }
}
